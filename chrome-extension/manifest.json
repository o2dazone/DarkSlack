{
  "manifest_version": 2,

  "name": "Dark Slack",
  "description": "Alterations to the Slack UI. Darken and alter many portions of the UI.",
  "version": "0.1",

  "permissions": [
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*.slack.com/*"],
      "css": ["./dark_slack.css"],
      "js": ["./index.js"]
    }
  ]
}